{
   "name": "backend",
   "version": "0.0.1",
   "description": "Projeto de banco de dados para mat√©ria da UnB.",
   "devDependencies": {
      "@babel/plugin-proposal-class-properties": "^7.16.7",
      "@babel/plugin-proposal-decorators": "^7.17.2",
      "@types/express": "^4.17.13",
      "@types/node": "^8.0.29",
      "dotenv-cli": "^5.0.0",
      "ts-node": "3.3.0",
      "ts-node-dev": "^1.1.8",
      "tsconfig-paths": "^3.13.0"
   },
   "dependencies": {
      "@types/cors": "^2.8.12",
      "cors": "^2.8.5",
      "dotenv": "^16.0.0",
      "express": "^4.17.3",
      "mysql": "^2.14.1",
      "reflect-metadata": "^0.1.10",
      "typeorm": "^0.2.25",
      "typescript": "^4.6.2"
   },
   "scripts": {
      "dev": "dotenv -- tsnd -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm --config ./src/Db/config.cli",
      "db:build": "yarn typeorm migration:run",
      "db:drop": "yarn typeorm schema:drop",
      "db:reset": "yarn db:drop && yarn db:build"
   }
}
